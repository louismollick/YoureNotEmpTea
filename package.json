  "bugs": {
    "url": "https://github.com/Ruiisuuu/YoureNotEmpTea/issues"
  },
  "homepage": "https://github.com/Ruiisuuu/YoureNotEmpTea#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.0.2",
    "dotenv-flow": "^3.1.0",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "mongoose": "^5.8.5",
    "node-fetch": "^2.6.0",
    "socket.io": "^2.5.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
